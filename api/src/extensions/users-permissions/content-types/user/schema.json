{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "avatar_url": {
      "type": "string",
      "regex": "        RegExp pattern: [(http(s)?):\\/\\/(www\\.)?a-zA-Z0-9@:%._\\+~#=]{2,256}\\.[a-z]{2,6}\\b([-a-zA-Z0-9@:%_\\+.~#?&//=]*)"
    },
    "Name": {
      "type": "string",
      "required": true
    },
    "last_name": {
      "type": "string",
      "required": true
    },
    "Phone": {
      "type": "biginteger",
      "required": true
    },
    "Adresse": {
      "type": "text",
      "required": true
    },
    "Region": {
      "type": "enumeration",
      "enum": [
        "Ariana",
        "BÃ©ja",
        "Ben arous",
        "Bizert",
        "Gabes",
        "Gafsa",
        "Jandouba",
        "Kairaoun",
        "Kassrine",
        "Kebili",
        "La Manouba",
        "Le Kef",
        "Mahdia",
        "Mednine",
        "Monastir",
        "Nabeul",
        "Sfax",
        "Sidi Bouzid",
        "Seliana",
        "Sousse",
        "Tatouine",
        "Tozeur",
        "Tunis",
        "Zaghouan"
      ],
      "required": true
    },
    "City": {
      "type": "string",
      "required": true
    },
    "Additional_information": {
      "type": "text"
    }
  }
}
